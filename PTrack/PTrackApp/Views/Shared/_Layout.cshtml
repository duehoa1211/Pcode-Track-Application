<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Login</title>

    <link href="~/Content/Flexbox/css/flexboxgrid.css" rel="stylesheet" />
    <link href="~/Content/Animate/animate.css" rel="stylesheet" />
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
    <link href="~/css/StyleSheet-Media.css" rel="stylesheet" />
    @RenderSection("css", required: false)
</head>
<body>

    <div class="container body-content">
        @RenderBody()
    </div>

    <footer class="animated fadeInUp">
        <p>"All rights reserved" (in Copyrights @@ PCODE TEAM 2018)</p>
    </footer>

    <script src="~/Content/Jquery/jquery-3.3.1.min.js"></script>
    <script src="~/js/JavaScript.js"></script>
    @RenderSection("js", required: false)
</body>
</html>
